{
    "rewrites": [
      { "source": "/", "destination": "/monn.html" }
    ],
    "headers": [
      {
        "source": "**/*.mov",
        "headers": [
          {
            "key": "Content-Type",
            "value": "video/quicktime"
          },
          {
            "key": "Accept-Ranges",
            "value": "bytes"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      },
      {
        "source": "**/*.mp4",
        "headers": [
          {
            "key": "Content-Type",
            "value": "video/mp4"
          },
          {
            "key": "Accept-Ranges",
            "value": "bytes"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      }
    ]
  }